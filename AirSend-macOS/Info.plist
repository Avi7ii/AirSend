<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE plist PUBLIC "-//Apple//DTD PLIST 1.0//EN" "http://www.apple.com/DTDs/PropertyList-1.0.dtd">
<plist version="1.0">
<dict>
    <key>CFBundleIdentifier</key>
    <string>com.example.AirSend-macOS</string>
    <key>CFBundleName</key>
    <string>AirSend</string>
    <key>CFBundlePackageType</key>
    <string>APPL</string>
    <key>CFBundleShortVersionString</key>
    <string>2.1</string>
    <key>LSUIElement</key>
    <true/>
    <key>NSPrincipalClass</key>
    <string>NSApplication</string>
    <key>CFBundleExecutable</key>
    <string>AirSend</string>
    <key>NSLocalNetworkUsageDescription</key>
    <string>LocalSend needs to access your local network to discover and transfer files to other devices.</string>
    <key>NSBonjourServices</key>
    <array>
        <string>_localsend._tcp</string>
        <string>_localsend._udp</string>
    </array>
    <key>NSAppTransportSecurity</key>
    <dict>
        <key>NSAllowsArbitraryLoads</key>
        <true/>
        <key>NSAllowsLocalNetworking</key>
        <true/>
    </dict>
    <key>CFBundleIconFile</key>
    <string>AppIcon.icns</string>
